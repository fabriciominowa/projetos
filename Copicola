<script>
  /* auto rezise window by iframe content */
    window.addEventListener("message", receiveMessage, false);
    function receiveMessage(event)
    {
        if(event.origin == "http://localhost" || event.origin == "https://011lab.prefeitura.sp.gov.br") {
            iframe = document.getElementById("global-bar");
            iframe.style.height = event.data + "px";

            RewriteEngine On
          RewriteCond %{HTTPS} on
           RewriteRule ^ http://https://copicola.prefeitura.sp.gov.br/{HTTP_HOST}%{REQUEST_URI} [L,R=301]
        }
        RewriteCond %{HTTPS} on
        RewriteRule (.*) http://%copicola.prefeitura.sp.gov.br/{HTTP_HOST}%{REQUEST_URI} [L]

    }

</script>
<iframe id="global-bar" src="https://011lab.prefeitura.sp.gov.br/header/global_header.php?color=white" onload="this.contentWindow.postMessage('height', 'https://011lab.prefeitura.sp.gov.br');" scrolling="no" width="100%" height="50px" frameBorder="0"></iframe>
